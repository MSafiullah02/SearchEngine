    "metadata": {
        "title": "What\u2019s new about circulating biomarkers in pediatric acute lung disease",
        "authors": [
            {
                "first": "Am\u00e9lia",
                "middle": [],
                "last": "Moreira",
                "suffix": "",
                "email": "Amelia.Moreira@hcuge.ch",
                "affiliation": {}
            },
            {
                "first": "Anil",
                "middle": [],
                "last": "Sapru",
                "suffix": "",
                "email": "anilsapru@ucla.edu",
                "affiliation": {}
            },
            {
                "first": "Peter",
                "middle": [
                    "C."
                ],
                "last": "Rimensberger",
                "suffix": "",
                "email": "Peter.Rimensberger@hcuge.ch",
                "affiliation": {}
