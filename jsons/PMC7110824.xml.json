    "metadata": {
        "title": "The epidemiology of Middle East respiratory syndrome coronavirus in the Kingdom of Saudi Arabia, 2012\u20132015",
        "authors": [
            {
                "first": "Abdullah",
                "middle": [
                    "J."
                ],
                "last": "Alsahafi",
                "suffix": "",
                "email": "Abdullah.sahafi@monash.edu",
