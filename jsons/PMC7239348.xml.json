    "metadata": {
        "title": "Possible prophylactic or preventive role of topical povidone iodine during accidental ocular exposure to 2019-nCoV",
        "authors": [
            {
                "first": "Phulen",
                "middle": [],
                "last": "Sarma",
                "suffix": "",
                "email": "phulen10@gmail.com",
                "affiliation": {}
