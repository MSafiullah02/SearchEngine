                },
                {
                    "start": 188,
                    "end": 190,
                    "mention": "3.",
                    "ref_id": "BIBREF14"
                },
                {
                    "start": 615,
                    "end": 617,
                    "mention": "4.",
                    "ref_id": "BIBREF15"
                },
                {
                    "start": 619,
                    "end": 621,
                    "mention": "5.",
                    "ref_id": "BIBREF16"
                },
                {
                    "start": 640,
                    "end": 641,
                    "mention": "6",
                    "ref_id": "BIBREF17"
                },
                {
                    "start": 671,
