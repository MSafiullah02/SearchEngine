    "metadata": {
        "title": "Trials on oxygen supplementation in sepsis: better late than never",
        "authors": [
            {
                "first": "Anders",
                "middle": [],
                "last": "Perner",
                "suffix": "",
                "email": "anders.perner@regionh.dk",
