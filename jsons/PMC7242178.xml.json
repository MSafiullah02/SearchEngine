    "metadata": {
        "title": "Downregulation of ACE2 induces overstimulation of the renin\u2013angiotensin system in COVID-19: should we block the renin\u2013angiotensin system?",
        "authors": [
            {
                "first": "Fran\u00e7ois",
                "middle": [],
                "last": "Silhol",
                "suffix": "",
                "email": "francois.silhol@ap-hm.fr",
                "affiliation": {}
            },
            {
                "first": "Gabrielle",
                "middle": [],
                "last": "Sarlon",
                "suffix": "",
                "email": null,
                "affiliation": {}
            },
            {
                "first": "Jean-Claude",
                "middle": [],
                "last": "Deharo",
                "suffix": "",
                "email": null,
                "affiliation": {}
            },
            {
                "first": "Bernard",
