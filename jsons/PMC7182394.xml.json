                },
                {
                    "start": 1524,
                    "end": 1528,
                    "mention": "2013",
                    "ref_id": "BIBREF2"
                }
            ],
            "section": "COVID-19 challenging cell biologists",
            "ref_spans": [
                {
                    "start": 927,
                    "end": 928,
                    "mention": "1",
                    "ref_id": "FIGREF0"
                }
            ]
        },
        {
            "text": "Like these, many other questions regarding virus-host cell interactions wait to be tackled and the editors of Protoplasma are excited to receive manuscripts dealing with these aspects.",
            "cite_spans": [],
            "section": "COVID-19 challenging cell biologists",
            "ref_spans": []
        }
    ],
