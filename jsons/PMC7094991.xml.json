    "metadata": {
        "title": "COVID-19 may transmit through aerosol",
        "authors": [
            {
                "first": "Juan",
                "middle": [],
                "last": "Wang",
                "suffix": "",
                "email": null,
                "affiliation": {}
            },
            {
                "first": "Guoqiang",
