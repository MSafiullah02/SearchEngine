    "metadata": {
        "title": "Switch from oral anticoagulants to parenteral heparin in SARS-CoV-2 hospitalized patients",
        "authors": [
            {
                "first": "Sophie",
                "middle": [],
                "last": "Testa",
                "suffix": "",
                "email": "s.testa@asst-cremona.it",
